{
  "name": "ufone-bsms-admin-portal",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "cross-env react-scripts build",
    "build:stag": "cross-env REACT_APP_ENV=staging react-scripts build",
    "build:prod": "cross-env REACT_APP_ENV=production react-scripts build",
    "test": "jest --colors --collectCoverageFrom []",
    "server": "nodemon --watch 'src/' --watch '__tests__/' -e js --exec node ./__tests__/mock-server/server.js",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc.json"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.20",
    "@hookform/resolvers": "^3.9.0",
    "@reduxjs/toolkit": "^2.2.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.51.9",
    "axios": "^1.7.2",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "echarts-for-react": "^3.0.2",
    "file-saver": "^2.0.5",
    "module-alias": "^2.2.3",
    "msw": "^2.4.2",
    "node-forge": "^1.3.1",
    "react": "^18.3.1",
    "react-date-range": "^2.0.1",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.52.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0",
    "react-scripts": "5.0.1",
    "react-side-effect": "^2.1.2",
    "react-simple-keyboard": "^3.8.39",
    "react-svg": "^16.1.34",
    "redux-persist": "^6.0.0",
    "sass": "^1.77.6",
    "sweetalert2": "^11.12.1",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "__tests__": "__tests__",
    "pages": "src/pages",
    "components": "src/js/components",
    "hooks": "src/hooks",
    "layout": "src/layout",
    "libs": "src/libs",
    "media": "src/media",
    "routes": "src/routes",
    "store": "src/store",
    "styles": "src/styles"
  }
}
